{
  "name": "stylific",
  "version": "0.3.0",
  "homepage": "http://mitranim.com/stylific/",
  "scripts": {
    "start": "gulp",
    "gulp": "gulp",
    "prepublish": "gulp lib:build --prod",
    "push": "gulp build --prod && git push origin $(git symbolic-ref --short HEAD)",
    "push-docs": "gulp build --prod && (cd stylific-gh-pages && git add -A . && git commit -a -m autocommit && git push origin gh-pages)"
  },
  "authors": [
    "Mitranim <me@mitranim.com>"
  ],
  "description": "Component-oriented, flexbox-based CSS library, written in Sass.",
  "main": "scss/stylific.scss",
  "keywords": [
    "css",
    "style",
    "scss",
    "flexbox"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mitranim/stylific.git"
  },
  "devDependencies": {
    "browser-sync": "^2.7.12",
    "gulp": "git://github.com/gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.0",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^1.2.5",
    "gulp-image-resize": "^0.6.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-marked": "^1.0.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-rimraf": "^0.1.1",
    "gulp-sass": "^2.0.1",
    "gulp-statil": "0.0.4",
    "gulp-watch": "^4.2.4",
    "highlight.js": "^8.6.0",
    "lodash": "^3.9.3",
    "yargs": "^3.13.0"
  }
}
